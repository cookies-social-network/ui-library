{
  "name": "@cookies-social-network/components",
  "private": false,
  "version": "0.0.8",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cookies-ui-lib.umd.cjs",
  "module": "./dist/cookies-ui-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cookies-ui-lib.js",
      "require": "./dist/cookies-ui-lib.umd.cjs",
      "styles": "./dist/style.css"
    },
    "./css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --strictPort",
    "build": "vite build",
    "preview": "vite preview --strictPort",
    "remote": "pnpm build && pnpm preview",
    "prepare": "husky",
    "eslint": "eslint . --ext .js,.tsx,.ts",
    "eslint:fix": "pnpm run eslint --fix",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "pnpm stylelint --fix",
    "lint": "pnpm eslint && pnpm run stylelint",
    "lint:fix": "pnpm eslint:fix && pnpm stylelint:fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "type-check": "tsc --noEmit --skipLibCheck",
    "unit": "vitest run",
    "unit:update": "vitest run -u",
    "unit:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.22",
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@playwright/test": "^1.42.1",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.3.1",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "happy-dom": "^13.6.2",
    "husky": "^9.0.11",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "storybook": "^8.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-lib-inject-css": "^2.0.0",
    "vitest": "^1.3.1"
  }
}
